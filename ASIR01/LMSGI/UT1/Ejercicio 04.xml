        <horas_semanales>4</horas_semanales>
        <caracter>obligatorio</caracter>
        <fecha_inicio dia="24" mes="octubre" año="2022">
        <fecha_fin dia="30" mes="junio" año="2023">

        <!-- Creamos la etiqueta para alojar la información referente a un -->
        <alumno nombre="Ana" apellidos="Fernández Gutiérrez">
            <!-- Datos personales del alumno -->
            <datos_personales dni="16965696L" telefono="656566555" email="ana.fdezgtrrez@hotmail.com"></datos_personales>
            <direccion calle="C/ El Percebe" numero="13" ciudad="Santander" codpostal="39302"></direccion>
